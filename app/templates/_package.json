{
  "name": "queo-<%= repositoryName %>",
  "version": "0.1.0",
  "description": "<%= appDescription %>",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "<%= appAuthorName %>",
    "email": "<%= appAuthorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://code.dev.queo-group.com/git/<%= repositoryName %>"
  },
  "scripts": {
    "development": "cd ./node_modules/gulp-starter && npm run-script development",
    "demo": "cd ./node_modules/gulp-starter && npm run-script demo",
    "production": "cd ./node_modules/gulp-starter && npm run-script production",
    "dist-patch": "cd ./node_modules/gulp-starter && npm run-script dist-patch",
    "dist-update": "cd ./node_modules/gulp-starter && npm run-script dist-update",
    "dist-upgrade": "cd ./node_modules/gulp-starter && npm run-script dist-upgrade"
  },
  "devDependencies": {
    "gulp-starter": "rcreate/gulp-starter#develop"
    <% if( cssPreprocessor == 'sass' ) { %>,"styleguide": "rcreate/styleguide"<% } %>
  },
  "dependencies": {
    "<%= bootstrapName %>": "<%= bootstrapVersion %>"
    <% if( cssPreprocessor == 'sass' ) { %>,"highlightjs": "8.7.0"<% } %>
    ,"jquery": "^3.1.0"
  },
  "engines": {
    "node": ">=4"
  }
}
