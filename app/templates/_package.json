{
  "name": "queo-<%= repositoryName %>",
  "version": "0.1.0",
  "description": "<%= appDescription %>",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "<%= appAuthorName %>",
    "email": "<%= appAuthorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://code.dev.queo-group.com/git/<%= repositoryName %>"
  },
  "scripts": {
    "development": "npm explore queo-blendid -- npm run development",
    "production": "npm explore queo-blendid -- npm run build",
    "dist-patch": "npm explore queo-blendid -- npm run dist-patch",
    "dist-update": "npm explore queo-blendid -- npm run dist-update",
    "dist-upgrade": "npm explore queo-blendid -- npm run dist-upgrade"
  },
  "devDependencies": {
    "queo-gulp-starter": "^3.3.1"
    <% if( cssPreprocessor == 'sass' && enableStyleguide === true ) { %>,"queo-styleguide": "^0.2.0"<% } %>
  },
  "dependencies": {
    "<%= bootstrapName %>": "<%= bootstrapVersion %>"
    <% if( cssPreprocessor == 'sass' && enableStyleguide === true ) { %>,"highlightjs": "8.7.0"<% } %>
    ,"jquery": "^3.1.0"
  },
  "engines": {
    "node": ">=4"
  }
}
